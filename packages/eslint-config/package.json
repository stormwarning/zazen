{
	"name": "@zazen/eslint-config",
	"version": "6.10.0",
	"description": "Lint JavaScript, free of weariness and confusion",
	"keywords": [
		"eslint-config",
		"xo"
	],
	"homepage": "https://github.com/stormwarning/zazen-eslint-config",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/stormwarning/zazen.git",
		"directory": "packages/eslint-config"
	},
	"license": "ISC",
	"author": "Jeff (https://tidaltheory.io)",
	"type": "module",
	"exports": {
		".": {
			"import": "./configs/index.js"
		},
		"./node": {
			"import": "./configs/node.js"
		},
		"./typescript": {
			"import": "./configs/typescript.js"
		}
	},
	"main": "index.js",
	"files": [
		"configs",
		"utils",
		"CHANGELOG.md",
		"LICENSE.txt",
		"README.md"
	],
	"scripts": {
		"inspect": "eslint --inspect-config",
		"lint": "eslint .",
		"test": "vitest run"
	},
	"dependencies": {
		"@eslint/eslintrc": "3.0.2",
		"@typescript-eslint/eslint-plugin": "7.7.1",
		"@typescript-eslint/parser": "7.7.1",
		"eslint-config-prettier": "9.1.0",
		"eslint-config-xo": "0.44.0",
		"eslint-config-xo-typescript": "4.0.0",
		"eslint-plugin-etc": "2.0.3",
		"eslint-plugin-import": "2.29.1",
		"eslint-plugin-import-sorting": "1.1.0",
		"eslint-plugin-n": "17.2.1",
		"eslint-plugin-prefer-let": "3.0.1",
		"eslint-plugin-promise": "6.1.1",
		"eslint-plugin-unicorn": "52.0.0",
		"globals": "15.0.0"
	},
	"devDependencies": {
		"ava": "5.3.1",
		"is-plain-obj": "4.1.0"
	},
	"peerDependencies": {
		"eslint": ">=8.57.0",
		"prettier": ">=2"
	},
	"engines": {
		"node": ">=18"
	}
}
