{
  "name": "zazen",
  "version": "0.0.2-0",
  "description": "To practice the Way singleheartedly is, in itself, enlightenment. There is no gap between practice and enlightenment or zazen and daily life.",
  "homepage": "https://github.com/stormwarning/zazen",
  "author": "Jeff (http://tidaltheory.co/)",
  "private": true,
  "config": {
    "commitizen": {
      "path": "emoji-cz",
      "emoji-cz": {
        "types": {
          "Style": {
            "emoji": "üëï",
            "name": "Code style",
            "description": "Improving structure/format of the code."
          },
          "Perf": {
            "emoji": "‚ö°Ô∏è",
            "name": "Perf",
            "description": "A code change that improves performance."
          },
          "Config": {
            "emoji": "‚öôÔ∏è",
            "name": "Config",
            "description": "Adjusting project configuration settings."
          },
          "Content": {
            "emoji": "‚úèÔ∏è",
            "name": "Content",
            "description": "Content edits or additions."
          },
          "Dependency": {
            "emoji": "üì¶",
            "name": "Dependency",
            "description": "Adding or up/downgrading project dependencies."
          },
          "First": {
            "emoji": "üéâ",
            "name": "Init",
            "description": "Initial commit."
          },
          "UI": {
            "emoji": "üé®",
            "name": "UI",
            "description": "Visual or interaction changes."
          }
        },
        "format": "[emoji] [subject]"
      }
    }
  },
  "dependencies": {
    "auto-bind": "^1.1.0",
    "cac": "^4.2.4",
    "camelcase": "^4.1.0",
    "github-username": "^4.1.0",
    "github-username-regex": "^1.0.0",
    "global": "^4.3.2",
    "is-email": "^1.0.0",
    "is-url": "^1.2.2",
    "is-valid-npm-name": "0.0.4",
    "npm-conf": "^1.1.3",
    "npm-name-exists": "^1.0.3",
    "sao": "^0.22.9",
    "semver": "^5.4.1",
    "spdx-license-list": "^3.0.1",
    "speakingurl": "^14.0.1",
    "superb": "^1.3.0",
    "update-notifier": "^2.3.0",
    "uppercamelcase": "^3.0.0"
  },
  "devDependencies": {
    "babel-eslint": "~7.2.3",
    "commitizen": "~2.9.6",
    "emoji-cz": "^0.3.0",
    "eslint": "~4.6.1",
    "eslint-config-standard": "~10.2.1",
    "eslint-plugin-import": "~2.7.0",
    "eslint-plugin-node": "~5.1.1",
    "eslint-plugin-promise": "~3.5.0",
    "eslint-plugin-standard": "~3.0.1",
    "husky": "^0.14.3",
    "lint-staged": "~4.3.0",
    "npm-run-all": "~4.1.2",
    "prettier-eslint-cli": "~4.4.0",
    "prettier-stylelint": "^0.4.2",
    "remark-cli": "~4.0.0",
    "remark-contributors": "~3.1.0",
    "remark-github": "~7.0.2",
    "remark-preset-lint-markdown-style-guide": "~2.1.1",
    "remark-toc": "~4.0.1"
  },
  "keywords": [],
  "license": "MIT",
  "lint-staged": {
    "*.md": [
      "remark . -qo",
      "git add"
    ]
  },
  "publishConfig": {
    "message": "üîñ %s"
  },
  "remarkConfig": {
    "plugins": [
      "preset-lint-markdown-style-guide",
      [
        "lint-maximum-line-length",
        false
      ],
      [
        "lint-list-item-spacing",
        false
      ],
      "contributors",
      "github",
      "toc"
    ],
    "settings": {
      "commonmark": true,
      "bullet": "-",
      "listItemIndent": 1,
      "rule": "-",
      "ruleSpaces": false,
      "strong": "*",
      "emphasis": "*"
    }
  },
  "repository": "stormwarning/zazen",
  "scripts": {
    "cz": "git-cz",
    "lint": "npm-run-all lint:*",
    "lint:markdown": "remark . -qo",
    "precommit": "lint-staged && npm test",
    "test": "npm run lint"
  }
}
